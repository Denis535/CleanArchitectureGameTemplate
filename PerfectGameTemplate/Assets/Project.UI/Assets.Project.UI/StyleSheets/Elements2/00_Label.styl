@import "../StyleSheet__Theme"

// label
skin--label = create-view-skin(null, (element element), (element element)), create-text(upper-left, null, null)

// label
misc--label = create-layout-skin(null, auto auto, 4px, 1px, 12px 12px 10px 10px), create-flex(0, 1, auto, auto)

// label
label(effect)
    add-skin effect, label
    add-misc effect, label

// label
.unity-label
    label(null)
