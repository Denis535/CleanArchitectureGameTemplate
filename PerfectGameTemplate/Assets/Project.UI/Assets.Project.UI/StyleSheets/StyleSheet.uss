.unity-card .unity-header {
  font-size: 140%;
  -unity-font-style: bold;
}
.unity-card .unity-content {
  font-size: 100%;
  -unity-font-style: normal;
}
.unity-card .unity-footer {
  font-size: 130%;
  -unity-font-style: bold;
}
.unity-dialog-card .unity-content,
.unity-info-dialog-card .unity-content,
.unity-warning-dialog-card .unity-content,
.unity-error-dialog-card .unity-content {
  font-size: 120%;
  -unity-font-style: bold;
}
.title {
  font-size: 120%;
  -unity-font-style: bold;
}
MainWidgetView {
  background-image: url('project://database/Assets/Project.UI/Assets.Project.UI.MainScreen/Images/Background.png');
  -unity-background-scale-mode: scale-and-crop;
  position: absolute;
  width: 100%;
  height: 100%;
  margin-left: 0px;
  margin-right: 0px;
  margin-top: 0px;
  margin-bottom: 0px;
  border-left-width: 0px;
  border-right-width: 0px;
  border-top-width: 0px;
  border-bottom-width: 0px;
  padding-left: 0px;
  padding-right: 0px;
  padding-top: 0px;
  padding-bottom: 0px;
  transition-property: scale, -unity-background-image-tint-color;
  transition-duration: 3s;
  transition-timing-function: ease;
}
