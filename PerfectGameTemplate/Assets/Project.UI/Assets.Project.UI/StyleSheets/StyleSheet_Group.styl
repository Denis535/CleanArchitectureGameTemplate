@import "StyleSheet_Internal"
@import "StyleSheet_Internal2"

// group
group()
    add-skin(group)
    add-skin2(group)
    &.light, &.light-0
        add-skin(light-group, alpha: 0.00)
    &.light-1
        add-skin(light-group, alpha: 0.10)
    &.light-2
        add-skin(light-group, alpha: 0.20)
    &.light-3
        add-skin(light-group, alpha: 0.30)
    &.light-4
        add-skin(light-group, alpha: 0.40)
    &.light-5
        add-skin(light-group, alpha: 0.50)
    &.light-6
        add-skin(light-group, alpha: 0.60)
    &.light-7
        add-skin(light-group, alpha: 0.70)
    &.light-8
        add-skin(light-group, alpha: 0.80)
    &.light-9
        add-skin(light-group, alpha: 0.90)
    &.light-10
        add-skin(light-group, alpha: 1.00)
    &.dark, &.dark-0
        add-skin(dark-group, alpha: 0.00)
    &.dark-1
        add-skin(dark-group, alpha: 0.10)
    &.dark-2
        add-skin(dark-group, alpha: 0.20)
    &.dark-3
        add-skin(dark-group, alpha: 0.30)
    &.dark-4
        add-skin(dark-group, alpha: 0.40)
    &.dark-5
        add-skin(dark-group, alpha: 0.50)
    &.dark-6
        add-skin(dark-group, alpha: 0.60)
    &.dark-7
        add-skin(dark-group, alpha: 0.70)
    &.dark-8
        add-skin(dark-group, alpha: 0.80)
    &.dark-9
        add-skin(dark-group, alpha: 0.90)
    &.dark-10
        add-skin(dark-group, alpha: 1.00)
    &.small
        add-skin2(small-group)
    &.medium
        add-skin2(medium-group)
    &.large
        add-skin2(large-group)
    add-view()
    add-layout()
column-group()
    add-flex(0, 1, auto, auto)
    add-flex2(column, flex-start, stretch)
row-group()
    add-flex(0, 1, auto, auto)
    add-flex2(row, flex-start, stretch)

// group
.group
    group()
.column-group
    column-group()
.row-group
    row-group()
