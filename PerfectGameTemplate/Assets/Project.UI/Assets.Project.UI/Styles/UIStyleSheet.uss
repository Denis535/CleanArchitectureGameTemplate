}
.modal-widget-view {
  position: absolute;
  left: 50%;
  top: 49%;
  width: 60%;
  height: 50%;
  margin-left: 0px;
  margin-right: 0px;
  margin-top: 0px;
  margin-bottom: 0px;
  border-left-width: 0px;
