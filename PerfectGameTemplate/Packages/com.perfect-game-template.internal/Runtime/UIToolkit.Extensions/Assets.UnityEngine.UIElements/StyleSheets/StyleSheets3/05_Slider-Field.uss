}
.unity-base-slider .unity-base-field__input #unity-dragger {
  background-color: #b1b3b4;
  border-left-color: rgba(99,77,54,0.5);
  border-right-color: rgba(99,77,54,0.5);
  border-top-color: rgba(99,77,54,0.5);
