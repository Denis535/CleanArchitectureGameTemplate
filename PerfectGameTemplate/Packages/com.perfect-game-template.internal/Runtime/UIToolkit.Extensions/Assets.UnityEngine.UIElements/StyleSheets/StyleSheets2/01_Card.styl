flush()

// layout
add-style(card---------------------normal) layout(null, auto, auto, (0px), (1px 1px 1px 1px), (0px  0px  0px  0px ))
add-style(header-------------------normal) layout(null, auto, auto, (0px), (0px 0px 0px 1px), (24px 24px 20px 20px))
add-style(content------------------normal) layout(null, auto, auto, (0px), (0px 0px 0px 0px), (24px 24px 20px 20px))
add-style(footer-------------------normal) layout(null, auto, auto, (0px), (0px 0px 1px 0px), (24px 24px 20px 20px))
add-style(footer-------------------normal, '%s *') layout(width: 25%)
flush()
// layout
add-style(dialog-card--------------normal) layout(null, auto, auto, (0px), (1px 1px 1px 1px), (0px  0px  0px  0px ))
add-style(dialog-header------------normal) layout(null, auto, auto, (0px), (0px 0px 0px 1px), (24px 24px 20px 20px))
add-style(dialog-content-----------normal) layout(null, auto, auto, (0px), (0px 0px 0px 0px), (24px 24px 20px 20px))
add-style(dialog-footer------------normal) layout(null, auto, auto, (0px), (0px 0px 1px 0px), (24px 24px 20px 20px))
