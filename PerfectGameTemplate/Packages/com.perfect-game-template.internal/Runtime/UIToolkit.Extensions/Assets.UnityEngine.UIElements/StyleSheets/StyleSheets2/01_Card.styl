@import "../Internal/_Theme"

// selector
selector--card--------------------normal = '.card'
selector--card--------------------hover  = '.card:hover'
selector--header------------------normal = '.card       .header'
selector--header------------------hover  = '.card:hover .header'
selector--content-----------------normal = '.card       .content'
selector--content-----------------hover  = '.card:hover .content'
selector--footer------------------normal = '.card       .footer'
selector--footer------------------hover  = '.card:hover .footer'
// selector
selector--dialog-card-------------normal = '.dialog-card'
selector--dialog-card-------------hover  = '.dialog-card:hover'
selector--dialog-header-----------normal = '.dialog-card       .header'
selector--dialog-header-----------hover  = '.dialog-card:hover .header'
selector--dialog-content----------normal = '.dialog-card       .content'
selector--dialog-content----------hover  = '.dialog-card:hover .content'
selector--dialog-footer-----------normal = '.dialog-card       .footer'
selector--dialog-footer-----------hover  = '.dialog-card:hover .footer'
// selector
selector--info-dialog-card--------normal = '.info-dialog-card'
selector--info-dialog-card--------hover  = '.info-dialog-card:hover'
selector--info-dialog-header------normal = '.info-dialog-card       .header'
selector--info-dialog-header------hover  = '.info-dialog-card:hover .header'
selector--info-dialog-content-----normal = '.info-dialog-card       .content'
selector--info-dialog-content-----hover  = '.info-dialog-card:hover .content'
selector--info-dialog-footer------normal = '.info-dialog-card       .footer'
selector--info-dialog-footer------hover  = '.info-dialog-card:hover .footer'
// selector
selector--warning-dialog-card-----normal = '.warning-dialog-card'
selector--warning-dialog-card-----hover  = '.warning-dialog-card:hover'
selector--warning-dialog-header---normal = '.warning-dialog-card       .header'
selector--warning-dialog-header---hover  = '.warning-dialog-card:hover .header'
selector--warning-dialog-content--normal = '.warning-dialog-card       .content'
selector--warning-dialog-content--hover  = '.warning-dialog-card:hover .content'
selector--warning-dialog-footer---normal = '.warning-dialog-card       .footer'
selector--warning-dialog-footer---hover  = '.warning-dialog-card:hover .footer'
// selector
selector--error-dialog-card-------normal = '.error-dialog-card'
selector--error-dialog-card-------hover  = '.error-dialog-card:hover'
selector--error-dialog-header-----normal = '.error-dialog-card       .header'
selector--error-dialog-header-----hover  = '.error-dialog-card:hover .header'
selector--error-dialog-content----normal = '.error-dialog-card       .content'
selector--error-dialog-content----hover  = '.error-dialog-card:hover .content'
selector--error-dialog-footer-----normal = '.error-dialog-card       .footer'
selector--error-dialog-footer-----hover  = '.error-dialog-card:hover .footer'

// view
add-style(card---------------------normal) theme(card---------------------normal) font(null, 100%, normal)
add-style(card---------------------hover ) theme(card---------------------hover )
add-style(header-------------------normal) theme(header-------------------normal) font(null, 130%, bold  )
add-style(header-------------------hover ) theme(header-------------------hover )
add-style(content------------------normal) theme(content------------------normal) font(null, 100%, normal)
add-style(content------------------hover ) theme(content------------------hover )
add-style(footer-------------------normal) theme(footer-------------------normal) font(null, 120%, bold  )
add-style(footer-------------------hover ) theme(footer-------------------hover )
flush()
// view
add-style(dialog-card--------------normal) theme(dialog-card--------------normal) font(null, null, null  )
add-style(dialog-card--------------hover ) theme(dialog-card--------------hover )
add-style(dialog-header------------normal) theme(dialog-header------------normal) font(null, null, null  )
add-style(dialog-header------------hover ) theme(dialog-header------------hover )
add-style(dialog-content-----------normal) theme(dialog-content-----------normal) font(null, 120%, bold  )
add-style(dialog-content-----------hover ) theme(dialog-content-----------hover )
add-style(dialog-footer------------normal) theme(dialog-footer------------normal) font(null, null, null  )
add-style(dialog-footer------------hover ) theme(dialog-footer------------hover )
flush()
// view
add-style(info-dialog-card---------normal) theme(info-dialog-card---------normal) font(null, null, null  )
add-style(info-dialog-card---------hover ) theme(info-dialog-card---------hover )
add-style(info-dialog-header-------normal) theme(info-dialog-header-------normal) font(null, null, null  )
add-style(info-dialog-header-------hover ) theme(info-dialog-header-------hover )
add-style(info-dialog-content------normal) theme(info-dialog-content------normal) font(null, 120%, bold  )
add-style(info-dialog-content------hover ) theme(info-dialog-content------hover )
