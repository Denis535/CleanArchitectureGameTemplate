add-style(warning-dialog-content---normal) layout(null, auto, auto, (0px), (0px 0px 0px 0px), (24px 24px 20px 20px))
add-style(warning-dialog-footer----normal) layout(null, auto, auto, (0px), (0px 0px 1px 0px), (24px 24px 20px 20px))
add-style(warning-dialog-footer----normal, '%s *') layout(width: 25%)
flush()
// layout
add-style(error-dialog-card--------normal) layout(null, auto, auto, (0px), (1px 1px 1px 1px), (0px  0px  0px  0px ))
add-style(error-dialog-header------normal) layout(null, auto, auto, (0px), (0px 0px 0px 1px), (24px 24px 20px 20px))
