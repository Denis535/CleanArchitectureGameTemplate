add-style(info-dialog-footer-------hover ) theme(info-dialog-footer-------hover )
flush()
// view
add-style(warning-dialog-card------normal) theme(warning-dialog-card------normal) font(null, null, null  )
add-style(warning-dialog-card------hover ) theme(warning-dialog-card------hover )
add-style(warning-dialog-header----normal) theme(warning-dialog-header----normal) font(null, null, null  )
add-style(warning-dialog-header----hover ) theme(warning-dialog-header----hover )
add-style(warning-dialog-content---normal) theme(warning-dialog-content---normal) font(null, 120%, bold  )
add-style(warning-dialog-content---hover ) theme(warning-dialog-content---hover )
add-style(warning-dialog-footer----normal) theme(warning-dialog-footer----normal) font(null, null, null  )
add-style(warning-dialog-footer----hover ) theme(warning-dialog-footer----hover )
flush()
// view
add-style(error-dialog-card--------normal) theme(error-dialog-card--------normal) font(null, null, null  )
add-style(error-dialog-card--------hover ) theme(error-dialog-card--------hover )
add-style(error-dialog-header------normal) theme(error-dialog-header------normal) font(null, null, null  )
add-style(error-dialog-header------hover ) theme(error-dialog-header------hover )
add-style(error-dialog-content-----normal) theme(error-dialog-content-----normal) font(null, 120%, bold  )
add-style(error-dialog-content-----hover ) theme(error-dialog-content-----hover )
